@model restaurangprojekt.Models.Booking

@{
    ViewData["Title"] = "Booking - Details";
}

<h2>Detaljer för bokning</h2>

<div>
    <h4>Booking</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            BookingID
        </dt>
        <dd class="col-sm-10">
            @Model.BookingID
        </dd>
        <dt class="col-sm-2">
            UserID
        </dt>
        <dd class="col-sm-10">
            @Model.UserID
        </dd>
        <dt class="col-sm-2">
            TableID_FK
        </dt>
        <dd class="col-sm-10">
            @Model.TableID_FK
        </dd>
        <dt class="col-sm-2">
            RoomID
        </dt>
        <dd class="col-sm-10">
            @Model.RoomID
        </dd>
        <dt class="col-sm-2">
            ReservedDate
        </dt>
        <dd class="col-sm-10">
            @Model.ReservedDate
        </dd>
        <dt class="col-sm-2">
            GuestCount
        </dt>
        <dd class="col-sm-10">
            @Model.GuestCount
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Tillbaka till listan</a>
</div>
